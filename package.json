{
  "name": "healing-music-hz",
  "version": "1.0.0",
  "description": "hz - Solfeggio Harmonics Music",
  "author": "seiyamaeda",
  "private": true,
  "scripts": {
    "dev_": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev-debug": "node --inspect-brk=9229 node_modules/nuxt/bin/nuxt",
    "dev": "npm run env-debug && nuxt --spa",
    "serve": "npm run build && firebase serve",
    "build": "npm run env-debug && nuxt build",
    "build-release": "npm run env-product && nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "env-product": "rm -f .env; ln -s .env-product .env; ",
    "env-debug": "rm -f .env; ln -s .env-dev .env; ",
    "deploy-release": "npm run env-product && npm run build-release && firebase deploy -P release --only=hosting; rm -f .env;",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.9.0",
    "firebase": "^7.6.1",
    "firebaseui": "^4.4.0",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "nuxt": "^2.0.0",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^8.0.0",
    "vue-youtube": "^1.4.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.9.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.16.4"
  }
}
